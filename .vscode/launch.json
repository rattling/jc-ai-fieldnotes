{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "AgentsVsWorkflows: Run (default)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/experiments/agents_vs_workflows/scripts/run_agents_vs_workflows.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "justMyCode": true
    },
    {
      "name": "AgentsVsWorkflows: Workflow only",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/experiments/agents_vs_workflows/scripts/run_agents_vs_workflows.py",
      "args": ["--mode", "workflow", "--limit", "10"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "justMyCode": true
    },
    {
      "name": "AgentsVsWorkflows: Agent only",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/experiments/agents_vs_workflows/scripts/run_agents_vs_workflows.py",
      "args": ["--mode", "agent", "--limit", "10"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "justMyCode": true
    },
    {
      "name": "AgentsVsWorkflows: Full eval",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/experiments/agents_vs_workflows/scripts/run_agents_vs_workflows.py",
      "args": ["--mode", "eval"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "justMyCode": true
    }
  ]
}
